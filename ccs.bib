keywords = {privacy,shadow dom,web security},
pages = {1028--1039},
title = {{ShadowCrypt: Encrypted Web Applications for Everyone}},
url = {http://dl.acm.org/citation.cfm?id=2660267.2660326},
year = {2014}
}
