author = {Henecka, Wilko and Sadeghi, Ahmad-Reza and Schneider, Thomas and Wehrenberg, Immo and Others},
journal = {ccs},
pages = {451--462},
title = {{TASTY: tool for automating secure two-party computations}},
year = {2010}
