title = {{Security Analysis of India's Electronic Voting Machines}},
url = {http://dl.acm.org/citation.cfm?id=1866307.1866309 http://indiaevm.org/evm{\{}{\_}{\}}tr2010.pdf http://indiaevm.org/evm{\{}{\_}{\}}tr2010.pdf$\backslash$nhttp://www.cse.umich.edu/{\{}{~}{\}}jhalderm/pub/papers/evm-ccs10.pdf$\backslash$nhttp://www.schneier.com/blog/archives/2010/05/security{\{}{\_}{\}}analys{\{}{\_}{\}}2.html$\backslash$nhttp://indiaevm.org$\backslash$nhttp://indiaevm.org/evm{\{}{\_}{\}}tr2010-jul29.pdf$\backslash$nhttp://portal.a},
