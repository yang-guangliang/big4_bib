@article{Ramchen2014,
abstract = {In this work we explore new techniques for building short signatures$\backslash$rfrom obfuscation. Our goals are twofold. First, we would like to$\backslash$rachieve short signatures with adaptive security proofs. Second, we$\backslash$rwould like to build signatures with fast signing, ideally$\backslash$rsignificantly faster than comparable signatures that are not based on$\backslash$robfuscation. The goal here is to create an "imbalanced" scheme where$\backslash$rsigning is fast at the expense of slower verification.$\backslash$r$\backslash$rWe develop new methods for achieving short and fully secure$\backslash$robfuscation-derived signatures. Our base signature scheme is built$\backslash$rfrom punctured programming and makes a novel use of the "prefix$\backslash$rtechnique" to guess a signature. We find that our initial scheme has$\backslash$rslower performance than comparable algorithms (e.g. EC-DSA). We find$\backslash$rthat the underlying reason is that the underlying PRG is called$\backslash$rl{\{}{\^{}}{\}}2 times for security parameter l.$\backslash$r$\backslash$rTo address this issue we construct a more efficient scheme by adapting the Goldreich-Goldwasser-Micali [GGM86] construction to form the basis for a new puncturable PRF. This puncturable PRF accepts$\backslash$rvariable-length inputs and has the property that evaluations on all$\backslash$rprefixes of a message can be efficiently pipelined. Calls to the$\backslash$rpuncturable PRF by the signing algorithm therefore make fewer$\backslash$rinvocations of the underlying PRG, resulting in reduced signing$\backslash$rcosts.$\backslash$r$\backslash$rWe evaluate our puncturable PRF based signature schemes using a$\backslash$rvariety of cryptographic candidates for the underlying PRG. We show$\backslash$rthat the resulting performance on message signing is competitive with$\backslash$rthat of widely deployed signature schemes.},
author = {Ramchen, Kim and Waters, Brent},
doi = {10.1145/2660267.2660306},
isbn = {9781450329576},
issn = {15437221},
journal = {ccs},
title = {{Fully Secure and Fast Signing from Obfuscation}},
year = {2014}
}
