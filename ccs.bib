title = {{WAPTEC: Whitebox Analysis of Web Applications for Parameter Tampering Exploit Construction}},
url = {http://www.google.co.kr/search?cx=w{\{}{\&}{\}}sourceid=chrome{\{}{\&}{\}}ie=UTF-8{\{}{\&}{\}}q="WAPTEC:+Whitebox+Analysis+of+Web+Applications+for+Parameter+Tampering+Exploit+Construction" papers3://publication/uuid/A0593EDD-00B4-4199-9315-7F940226984F},
