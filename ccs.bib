@article{Kamara2012a,
abstract = {Secure function evaluation (SFE) allows a set of mutually distrustful parties to$\backslash$nevaluate a function of their joint inputs without revealing their inputs$\backslash$nto each other.$\backslash$nSFE has been the focus of active research and recent work suggests that it can$\backslash$nbe made practical. Unfortunately, current protocols and implementations have$\backslash$ninherent limitations that are hard to overcome using standard and practical$\backslash$ntechniques. Among them are: (1) requiring participants to do work linear in$\backslash$nthe size of the circuit representation of the function; (2) requiring all$\backslash$nparties to do the same amount of work; and (3) not being able to provide$\backslash$ncomplete fairness.$\backslash$n$\backslash$nA promising approach for overcoming these limitations is to augment the SFE$\backslash$nsetting with a small set of untrusted servers that have no input to the$\backslash$ncomputation and that receive no output, but that make their computational$\backslash$nresources available to the parties. In this model, referred to as$\backslash$nserver-aided SFE, the goal is to tradeoff the parties' work at the$\backslash$nexpense of the servers. Motivated by the emergence of public cloud services$\backslash$nsuch as Amazon EC2 and Microsoft Azure, recent work has explored the extent to$\backslash$nwhich server-aided SFE can be achieved with a single server.$\backslash$n$\backslash$nIn this work, we revisit the sever-aided setting from a practical perspective$\backslash$nand design single-server-aided SFE protocols that are considerably more$\backslash$nefficient than all previously-known protocols. We achieve this in part by$\backslash$nintroducing several new techniques for garbled-circuit-based protocols,$\backslash$nincluding a new and efficient input-checking mechanism for cut-and-choose and a$\backslash$nnew pipelining technique that works in the presence of malicious adversaries.$\backslash$nFurthermore, we extend the server-aided model to guarantee fairness which is an$\backslash$nimportant property to achieve in practice.$\backslash$n$\backslash$nFinally, we implement and evaluate our constructions experimentally and show$\backslash$nthat our protocols (regardless of the number of parties involved) yield implementations that are 4 and 6 times$\backslash$nfaster than the most optimized two-party SFE implementation when the server is$\backslash$nassumed to be malicious and covert, respectively.},
author = {Kamara, Seny and Mohassel, Payman and Riva, Ben},
doi = {10.1145/2382196.2382280},
isbn = {9781450316514},
issn = {15437221},
journal = {ccs},
keywords = {cloud computing,computation,multi-party,secure computation,server-aided computation},
pages = {797--808},
title = {{Salus: a system for server-aided secure function evaluation}},
url = {http://dl.acm.org/citation.cfm?id=2382280},
year = {2012}
}
@article{Calvet2012,
abstract = {Analyzing cryptographic implementations has important ap- plications, especially for malware analysis where they are an integral part both of the malware payload and the un- packing code that decrypts this payload. These implemen- tations are often based on well-known cryptographic func- tions, whose description is publicly available. While po- tentially very useful for malware analysis, the identification of such cryptographic primitives is made difficult by the fact that they are usually obfuscated. Current state-of-the- art identification tools are ineffective due to the absence of easily identifiable static features in obfuscated code. How- ever, these implementations still maintain the input-output (I/O) relationship of the original function. In this paper, we present a tool that leverages this fact to identify crypto- graphic functions in obfuscated programs, by retrieving their I/O parameters in an implementation-independent fashion, and comparing them with those of known cryptographic functions. In experimental evaluation, we successfully iden- tified the cryptographic functions TEA, RC4, AES andMD5 both in synthetic examples protected by a commercial-grade packer (AsProtect), and in several obfuscated malware sam- ples (Sality, Waledac, Storm Worm and SilentBanker). In addition, our tool was able to recognize basic operations done in asymmetric ciphers such as RSA.},
author = {Calvet, Joan and Fernandez, Jos{\'{e}} M and Marion, Jean-yves},
doi = {10.1145/2382196.2382217},
isbn = {9781450316514},
issn = {15437221},
journal = {ccs},
keywords = {binary program analysis,cryptography,malware},
pages = {2--4},
title = {{Aligot : Cryptographic Function Identification in Obfuscated Binary Programs}},
year = {2012}
}
@article{Zhang2012,
abstract = {This paper details the construction of an access-driven side- channel attack by which a malicious virtual machine (VM) extracts fine-grained information from a victim VM running on the same physical computer. This attack is the first such attack demonstrated on a symmetric multiprocessing system virtualized using a modern VMM (Xen). Such systems are very common today, ranging from desktops that use virtualization to sandbox application or OS compromises, to clouds that co-locate the workloads of mutually distrustful customers. Constructing such a side-channel requires overcoming challenges including core migration, numerous sources of channel noise, and the difficulty of preempting the victim with sufficient frequency to extract fine-grained information from it. This paper addresses these challenges and demonstrates the attack in a lab setting by extracting an ElGamal decryption key from a victim using the most recent version of the libgcrypt cryptographic library.},
author = {Zhang, Yinqian and Juels, Ari and Reiter, Mk and Ristenpart, Thomas},
