isbn = {9781450338325},
journal = {ccs},
keywords = {D4 [Operating Sys-tems],Event Correlation,Function Call,Long Execution Path,Machine Learning,Program Attack,Security and Protection},
title = {{Unearthing Stealthy Program Attacks Buried in Extremely Long Execution Paths}},
