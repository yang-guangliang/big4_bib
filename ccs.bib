year = {2011}
}
@article{Zheng2011,
abstract = {Biometric authentication verifies a user based on its inherent, unique$\backslash$ncharacteristics --- who you are. In addition to physiological biometrics,$\backslash$nbehavioral biometrics has proven very useful in authenticating a$\backslash$nuser. Mouse dynamics, with their unique patterns of mouse movements,$\backslash$nis one such behavioral biometric. In this paper, we present a user$\backslash$nverification system using mouse dynamics, which is both accurate$\backslash$nand efficient enough for future usage. The key feature of our system$\backslash$nlies in using much more fine-grained (point-by-point) angle-based$\backslash$nmetrics of mouse movements for user verification. These new metrics$\backslash$nare relatively unique from person to person and independent of the$\backslash$ncomputing platform. Moreover, we utilize support vector machines$\backslash$n(SVMs) for accurate and fast classification. Our technique is robust$\backslash$nacross different operating platforms, and no specialized hardware$\backslash$nis required. The efficacy of our approach is validated through a$\backslash$nseries of experiments. Our experimental results show that the proposed$\backslash$nsystem can verify a user in an accurate and timely manner, and induced$\backslash$nsystem overhead is minor.},
author = {Zheng, Nan and Paloski, Aaron and Wang, Haining},
doi = {10.1145/2046707.2046725},
